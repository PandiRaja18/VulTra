{
  "name": "zacksaw",
  "version": "1.0.5",
  "displayName": "Zacksaw code analyser",
  "description": "Java Security Vulnerability Analyzer for Vscode IDE",
  "publisher": "your-publisher-name",
  "engines": {
    "vscode": "^1.74.0"
  },
  "main": "out/extension.js",
  "devDependencies": {
    "@types/node": "^16.18.0",
    "@types/vscode": "^1.74.0",
    "nodemon": "^2.0.3",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@xenova/transformers": "^2.17.2"
  },
  "scripts": {
    "build": "tsc",
    "dev": "nodemon --watch src -e ts,tsx,json,js --exec \"npm run build\"",
    "watch": "tsc --watch",
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "start": "node out/extension.js"
  },
  "activationEvents": [
    "onCommand:javaVulnerabilityAnalyzer.showPanel",
    "onCommand:javaVulnerabilityAnalyzer.analyzeWorkspace"
  ],
  "contributes": {
    "commands": [
      {
        "command": "javaVulnerabilityAnalyzer.showPanel",
        "title": "Show Vulnerability Panel",
        "category": "Java Security"
      },
      {
        "command": "javaVulnerabilityAnalyzer.analyzeWorkspace",
        "title": "Analyze Java Files",
        "category": "Java Security"
      }
    ],
    "keybindings": [
      {
        "command": "javaVulnerabilityAnalyzer.analyzeWorkspace",
        "key": "ctrl+alt+v",
        "when": "editorTextFocus && resourceExtname == '.java'"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "javaVulnerabilityAnalyzer.analyzeWorkspace",
          "when": "resourceExtname == .java",
          "group": "navigation"
        }
      ],
      "editor/context": [
        {
          "command": "javaVulnerabilityAnalyzer.analyzeWorkspace",
          "when": "resourceExtname == .java",
          "group": "navigation"
        }
      ]
    }
  }
}
